<?xml version="1.0" encoding="utf-8"?>
<merge
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/incidentView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:visibility="visible">


    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <TextView
                android:id="@+id/iw_positionLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="28dp"
                android:clickable="false"
                android:text="Plats:"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <TextView
                android:id="@+id/iw_timeLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="52dp"
                android:layout_marginLeft="52dp"
                android:layout_marginTop="24dp"
                android:clickable="false"
                android:text="Tid:"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iw_positionLabel"/>

        <TextView
                android:id="@+id/iw_countLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="24dp"
                android:clickable="false"
                android:text="Antal:"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iw_timeLabel"/>

        <TextView
                android:id="@+id/iw_locationText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="192dp"
                android:layout_marginRight="192dp"
                android:clickable="false"
                android:contentDescription="PLATS"
                android:text="PLATS???"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <TextView
                android:id="@+id/iw_timeText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="192dp"
                android:layout_marginRight="192dp"
                android:clickable="false"
                android:text="HH:MM"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iw_locationText"/>

        <TextView
                android:id="@+id/iw_countText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="192dp"
                android:layout_marginRight="192dp"
                android:clickable="false"
                android:text="X st"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iw_timeText"/>

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/horizontal_line"
                android:id="@+id/iw_line"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/iw_countText" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>

        <ImageView
                android:layout_width="35dp"
                android:layout_height="35dp" app:srcCompat="@drawable/shield"
                android:id="@+id/iw_endorseShield"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="25dp" android:layout_marginEnd="25dp" android:layout_marginRight="25dp"/>


    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>