<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:app="http://schemas.android.com/apk/res-auto"
       xmlns:tools="http://schemas.android.com/tools"
       android:id="@+id/userReportView"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       tools:visibility="visible">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <TextView
                android:id="@+id/urw_reportPositionLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="28dp"
                android:clickable="false"
                android:text="Plats:"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <TextView
                android:id="@+id/urw_timeLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="52dp"
                android:layout_marginLeft="52dp"
                android:layout_marginTop="24dp"
                android:clickable="false"
                android:text="Tid:"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/urw_reportPositionLabel"/>

        <TextView
                android:id="@+id/urw_countLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="24dp"
                android:clickable="false"
                android:text="Antal:"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/urw_timeLabel"/>

        <Button
                android:id="@+id/urw_editButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="28dp"
                android:layout_marginRight="28dp"
                android:text="Komplettera"
                app:layout_constraintEnd_toEndOf="parent" tools:layout_editor_absoluteY="100dp"/>
        <TextView
                android:id="@+id/urw_locationText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="192dp"
                android:layout_marginRight="192dp"
                android:clickable="false"
                android:contentDescription="PLATS"
                android:text="PLATS"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <TextView
                android:id="@+id/urw_timeText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="192dp"
                android:layout_marginRight="192dp"
                android:clickable="false"
                android:text="HH:MM"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/urw_locationText"/>

        <TextView
                android:id="@+id/urw_countText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="192dp"
                android:layout_marginRight="192dp"
                android:clickable="false"
                android:text="X st"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/urw_timeText"/>

        <ImageView
                android:id="@+id/urw_line"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.609"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/horizontal_line" android:layout_marginTop="15dp"
                app:layout_constraintTop_toBottomOf="@+id/urw_editButton"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</merge>